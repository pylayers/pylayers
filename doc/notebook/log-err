for cutoff in range(maxcutoff):
     del Si
        
     Si = Signatures(L,0,1)
     tic1=time.time()
     Si.run1(cutoff=cutoff)
     tic2=time.time()
     r2d = Si.rays(ptx,prx)   
     tic3=time.time()
     Si.num()   
     Nr = len(r2d)   
     tt1[cutoff]=tic2-tic1
     tt2[cutoff]=tic3-tic2   
     tint[cutoff]=Si.nint   
     tsig[cutoff]=Si.nsig
     tray[cutoff]=Nr
     print cutoff,tt2[cutoff]
#    L.display['ednodes']=False
#    r2d.show(L)

