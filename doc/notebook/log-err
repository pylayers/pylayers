rx=array([767,1114,1.5])
#
S.tx.point(tx)
S.rx.point(rx)
# getting cycles from tx 
ctx = S.L.pt2cy(S.tx.position[:,0])
# getting cycles from rx 
crx = S.L.pt2cy(S.rx.position[:,0])
f,a = S.show()


